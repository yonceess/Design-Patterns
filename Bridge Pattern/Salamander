public class Salamander extends Amphibians{

    public Salamander(){
        super("Salamander");
    }
    @Override
    public void output_info() {
        System.out.println("Now " + type +" spending time in");
        this.accommodation.output_type();

    }
    @Override
    public void breath_type() {
        this.breathing.breathe_type();
    }
}
